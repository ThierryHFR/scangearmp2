#: ../src/cnmsstrings.h:108 ../scangearmp2.glade.h:14
msgid "Save"
msgstr "Speichern"

#. STR_CNMS_LS_008_03
#: ../src/cnmsstrings.h:110
msgid "The file name is not specified."
msgstr "Der Dateiname wurde nicht angegeben."

#. STR_CNMS_LS_008_04
#: ../src/cnmsstrings.h:112
msgid ""
"The same file name already exists.\n"
"Are you sure to overwrite it?"
msgstr ""
"Derselbe Dateiname ist bereits vorhanden.\n"
"Möchten Sie ihn wirklich überschreiben?"

#. STR_CNMS_LS_008_05
#: ../src/cnmsstrings.h:114
msgid ""
"You do not have permission to access the specified file.\n"
"Specify another file name or save the file in another directory."
msgstr ""
"Sie haben keine Zugriffsberechtigung für die angegebene Datei.\n"
"Geben Sie einen anderen Dateinamen an, oder speichern Sie die Datei in einem \n"
"anderen Verzeichnis."

#. STR_CNMS_LS_008_06
#: ../src/cnmsstrings.h:116
msgid ""
"The specified directory does not exist or you do not have permission to "
"access the directory.\n"
"Save the file in another directory."
msgstr ""
"Das angegebene Verzeichnis ist nicht vorhanden, oder Sie haben keine \n"
"Zugriffsberechtigung dafür.\n"
"Speichern Sie die Datei in einem anderen Verzeichnis."

#. STR_CNMS_LS_008_07
#: ../src/cnmsstrings.h:118
msgid ""
"The specified file name is invalid.\n"
"Specify another file name."
msgstr ""
"Der angegebene Dateiname ist ungültig.\n"
"Geben Sie einen anderen Dateinamen an."

#. STR_CNMS_LS_009_01
#: ../src/cnmsstrings.h:120
msgid "Scanning..."
msgstr "Scanvorgang läuft..."

#. STR_CNMS_LS_009_02
#: ../src/cnmsstrings.h:122
msgid "Saving..."
msgstr "Speichern..."

#. STR_CNMS_LS_009_03
#: ../src/cnmsstrings.h:124
msgid "Saved the scan result."
msgstr "Scanergebnis gespeichert."

#. STR_CNMS_LS_009_04
#: ../src/cnmsstrings.h:126
#, c-format
msgid "%d page(s)"
msgstr "%d Seite(n)"

#. STR_CNMS_LS_009_05
#: ../src/cnmsstrings.h:128
msgid "Canceling..."
msgstr "Wird abgebrochen..."

#. STR_CNMS_LS_010_01
#: ../src/cnmsstrings.h:130
msgid "Scan has been canceled."
msgstr "Scanvorgang wurde abgebrochen."

#. STR_CNMS_LS_010_02
#: ../src/cnmsstrings.h:132
msgid "Scan has been canceled. Check the printer."
msgstr "Scanvorgang wurde abgebrochen. Überprüfen Sie den Drucker."

#. STR_CNMS_LS_010_03
#: ../src/cnmsstrings.h:134
msgid ""
"The printer is in operation. Check the printer.\n"
"Scanner driver will be closed."
msgstr ""
"Der Drucker ist in Betrieb. Überprüfen Sie den Drucker.\n"
"Der Scanner-Treiber wird geschlossen."

#. STR_CNMS_LS_010_04
#: ../src/cnmsstrings.h:136
msgid ""
"An error has occurred while scanning. Check the printer.\n"
"Scanner driver will be closed."
msgstr ""
"Es ist ein Fehler beim Scannen aufgetreten. Überprüfen Sie den Drucker.\n"
"Der Scanner-Treiber wird geschlossen."

#. STR_CNMS_LS_010_05
#: ../src/cnmsstrings.h:138
msgid ""
"Cannot communicate with scanner.\n"
"Scanner driver will be closed."
msgstr ""
"Kommunikation mit dem Scanner nicht möglich.\n"
"Der Scanner-Treiber wird geschlossen."

#. STR_CNMS_LS_010_06
#: ../src/cnmsstrings.h:140
msgid ""
"There is not enough free disk space to complete the scan.\n"
"Scanner driver will be closed."
msgstr ""
"Stellen Sie mehr Datenträgerspeicher zur Verfügung. \n"
"Der Scanner-Treiber wird geschlossen."

#. STR_CNMS_LS_010_07
#: ../src/cnmsstrings.h:142
msgid ""
"Cannot find available scanners.\n"
"\n"
"Cable may be disconnected or scanner may be turned off.\n"
"Check the scanner status, and then try again."
msgstr ""
"Es wurde kein verfügbarer Scanner gefunden.\n"
"\n"
"Möglicherweise ist das Kabel nicht angeschlossen oder der Scanner ist \n"
"ausgeschaltet.\n"
"Überprüfen Sie den Scanner-Status, und versuchen Sie es anschließend erneut."

#. STR_CNMS_LS_010_08
#: ../src/cnmsstrings.h:144
msgid ""
"The ADF (Auto Document Feeder) is empty. \n"
"Place paper in the ADF."
msgstr ""
"Der ADF (Automatischer Dokumenteneinzug) ist leer. \n"
"Legen Sie Papier in den ADF ein."

#. STR_CNMS_LS_010_09
#: ../src/cnmsstrings.h:146
msgid ""
"Internal Error occurred.\n"
"Scanner driver will be closed."
msgstr ""
"Es ist ein interner Fehler aufgetreten.\n"
"Der Scanner-Treiber wird geschlossen."

#. STR_CNMS_LS_011_01
#: ../src/cnmsstrings.h:148 ../scangearmp2.glade.h:26
msgid "Select Scanner"
msgstr "Scanner auswählen"

#. STR_CNMS_LS_011_02
#: ../src/cnmsstrings.h:150 ../scangearmp2.glade.h:8
msgid "No scanners detected."
msgstr "Keine Scanner erkannt."

#. STR_CNMS_LS_011_03
#: ../src/cnmsstrings.h:152 ../scangearmp2.glade.h:30
msgid "Update Scanner List"
msgstr "Scanner-Liste aktualisieren"

#. STR_CNMS_LS_011_04
#: ../src/cnmsstrings.h:154 ../scangearmp2.glade.h:10
msgid "OK"
msgstr "OK"

#. STR_CNMS_LS_011_05
#: ../src/cnmsstrings.h:156 ../scangearmp2.glade.h:2
msgid "Cancel"
msgstr "Abbrechen"

#. STR_CNMS_LS_012_01
#: ../src/cnmsstrings.h:158
msgid "Searching..."
msgstr "Suche läuft..."

#. STR_CNMS_LS_012_02
#: ../src/cnmsstrings.h:160
msgid "Searching for scanners."
msgstr "Suche nach Scannern wird ausgeführt."

#. STR_CNMS_LS_012_03
#: ../src/cnmsstrings.h:162
msgid "Updating the scanner list."
msgstr "Scanner-Liste wird aktualisiert."

#. STR_CNMS_LS_010_10
#: ../src/cnmsstrings.h:164
msgid ""
"Release the lock switch, detach the USB cable and reconnect.\n"
"Scanner driver will be closed."
msgstr ""
"Deaktivieren Sie den Sperrschalter, ziehen Sie das USB-Kabel ab und schließen Sie es erneut an.\n"
"Der Scanner-Treiber wird geschlossen."

#. STR_CNMS_LS_010_11
#: ../src/cnmsstrings.h:166
msgid "Scanner initialization is in progress. Scan after it is complete."
msgstr "Der Scanner wird gerade initialisiert. Scannen Sie nach diesem Vorgang."

